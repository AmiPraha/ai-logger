{
    "name": "ami-praha/ai-logger",
    "description": "A Laravel package that provides a logging driver to send logs to a specified external webhook.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Miloslav Janou≈°ek",
            "email": "miloslav.janousek@ami.cz"
        }
    ],
    "require": {
        "php": "^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "monolog/monolog": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "AmiPraha\\AILogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmiPraha\\AILogger\\AILoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}